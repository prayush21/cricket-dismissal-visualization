{
    "private": true,
    "workspaces": [
        "apps/frontend"
    ],
    "scripts": {
        "dev": "concurrently \"npm:dev --prefix apps/frontend\" \"python apps/backend/app.py\"",
        "build": "npm run build --prefix apps/frontend",
        "start:frontend": "npm start --prefix apps/frontend",
        "start:backend": "python apps/backend/app.py",
        "start": "npm run start:frontend & npm run start:backend"
        
    },
    "devDependencies": {
        "concurrently": "^9.0.1"
    },
    "dependencies": {
        "chart.js": "^4.4.4",
        "react-chartjs-2": "^5.2.0"
    }
}
